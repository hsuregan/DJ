<%= form_for @request, :url => url_for(:controller => 'welcome') do |f| %>
    <%= f.label :title %>
    <div class="controls">
    <%= f.text_field :title %>
    </div>

    <%= f.label :requester %>
    <div class="controls">
    <%= f.text_field :requester %>
    </div>


     <div class="form-actions">
    <%= f.submit nil %>
    <%= link_to 'Cancel', root_path %>
  </div>
<%end%>